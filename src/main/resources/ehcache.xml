<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd"
	updateCheck="true" monitoring="autodetect" dynamicConfig="true" name="RestCache">

	<cache name="movies" maxElementsInMemory="15" eternal="false"
		overflowToDisk="false" timeToLiveSeconds="10" timeToIdleSeconds="0"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
	</cache>
	
	  <cache name="comments" maxElementsInMemory="15" eternal="false"
		overflowToDisk="false" timeToLiveSeconds="10" timeToIdleSeconds="0"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
	</cache>  
</ehcache>